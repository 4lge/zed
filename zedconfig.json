{
    "preferences": {
        "tabSize": 4,
        "gotoExclude": ["/release/zed-*", "/nw/build/*", "/nw/download/*"]
    },
    "modes": {
        "json": {
            "commands": {
                "Development:Update Package.json File List": {
                    "scriptUrl": "/default/command/update_package.js"
                }
            }
        },
        "javascript": {
            "commands": {
                "Tools:Check": {
                    "options": {
                        "globals": {
                            "define": true,
                            "zed": true,
                            "nodeRequire": true,
                            "_": true,
                            "$": true,
                            "ace": true,
                            "Promise": true,
                            "sandboxRequest": true
                        }
                    }
                }
            }
        }
    }
}
